		<properties node_id="4" format="literal">{'controlAreaVisible': True, 'currentScriptIndex': 0, 'savedWidgetGeometry': b'\x01\xd9\xd0\xcb\x00\x03\x00\x00\x00\x00\x00\xef\x00\x00\x00B\x00\x00\x04\x10\x00\x00\x02@\x00\x00\x00\xf0\x00\x00\x00`\x00\x00\x04\x0f\x00\x00\x02?\x00\x00\x00\x00\x00\x00\x00\x00\x05\x00\x00\x00\x00\xf0\x00\x00\x00`\x00\x00\x04\x0f\x00\x00\x02?', 'scriptLibrary': [{'name': 'Table from numpy', 'script': '\nimport tkinter as tk\nfrom tkinter import ttk, messagebox\nimport Orange\nimport pickle\n\n# --------------------------------\n# LOAD MODEL\n# --------------------------------\ntry:\n    model = pickle.load(open("model.pkcls", "rb"))\nexcept:\n    messagebox.showerror("Error", "Model file \'lung_model.pkcls\' not found!")\n    exit()\n\n# --------------------------------\n# DOMAIN\n# --------------------------------\ndomain = Orange.data.Domain([\n    Orange.data.DiscreteVariable("GENDER", values=["1", "2"]),\n    Orange.data.ContinuousVariable("AGE"),\n    Orange.data.DiscreteVariable("SMOKING", values=["1", "2"]),\n    Orange.data.DiscreteVariable("YELLOW_FINGERS", values=["1", "2"]),\n    Orange.data.DiscreteVariable("ANXIETY", values=["1", "2"]),\n    Orange.data.DiscreteVariable("PEER_PRESSURE", values=["1", "2"]),\n    Orange.data.DiscreteVariable("CHRONIC_DISEASE", values=["1", "2"]),\n    Orange.data.DiscreteVariable("FATIGUE", values=["1", "2"]),\n    Orange.data.DiscreteVariable("ALLERGY", values=["1", "2"]),\n    Orange.data.DiscreteVariable("WHEEZING", values=["1", "2"]),\n    Orange.data.DiscreteVariable("ALCOHOL_CONSUMING", values=["1", "2"]),\n    Orange.data.DiscreteVariable("COUGHING", values=["1", "2"]),\n    Orange.data.DiscreteVariable("SHORTNESS_OF_BREATH", values=["1", "2"]),\n    Orange.data.DiscreteVariable("SWALLOWING_DIFFICULTY", values=["1", "2"]),\n    Orange.data.DiscreteVariable("CHEST_PAIN", values=["1", "2"]),\n], Orange.data.DiscreteVariable("LUNG_CANCER", values=["1", "2"]))\n\n\n# --------------------------------\n# MAIN WINDOW\n# --------------------------------\nroot = tk.Tk()\nroot.title("Lung Cancer Predictor")\nroot.geometry("360x500")\nroot.resizable(False, False)\n\n# --------------------------------\n# SCROLLABLE FRAME SETUP\n# --------------------------------\nmain_frame = tk.Frame(root)\nmain_frame.pack(fill=tk.BOTH, expand=1)\n\ncanvas = tk.Canvas(main_frame)\ncanvas.pack(side=tk.LEFT, fill=tk.BOTH, expand=1)\n\nscrollbar = ttk.Scrollbar(main_frame, orient=tk.VERTICAL, command=canvas.yview)\nscrollbar.pack(side=tk.RIGHT, fill=tk.Y)\n\ncanvas.configure(yscrollcommand=scrollbar.set)\ncanvas.bind("&lt;Configure&gt;", lambda e: canvas.configure(scrollregion=canvas.bbox("all")))\n\nframe = tk.Frame(canvas)\ncanvas.create_window((0, 0), window=frame, anchor="nw")\n\n\n# Smooth mouse scroll support\ndef on_mousewheel(event):\n    canvas.yview_scroll(int(-1 * (event.delta / 120)), "units")\n\ncanvas.bind_all("&lt;MouseWheel&gt;", on_mousewheel)\n\n\n# --------------------------------\n# INPUT FIELDS\n# --------------------------------\ntitle = tk.Label(frame, text="PulmoPredict.AI",\n                 bg="white", fg="red",\n                 font=("Arial", 16, "bold"))\ntitle.pack(pady=10)\n\nfields = {}\nquestions = [\n    "Gender (1=F, 2=M)",\n    "Age",\n    "Smoking",\n    "Yellow Fingers",\n    "Anxiety",\n    "Peer Pressure",\n    "Chronic Disease",\n    "Fatigue",\n    "Allergy",\n    "Wheezing",\n    "Alcohol Consuming",\n    "Coughing",\n    "Shortness of Breath",\n    "Swallowing Difficulty",\n    "Chest Pain"\n]\n\nyes_no = ["1", "2"]\n\nfor q in questions:\n    tk.Label(frame, text=q, font=("Arial", 11), anchor="w").pack(pady=2)\n\n    if q == "Age":\n        entry = tk.Entry(frame)\n    elif q.startswith("Gender"):\n        entry = ttk.Combobox(frame, values=["1", "2"], state="readonly")\n    else:\n        entry = ttk.Combobox(frame, values=yes_no, state="readonly")\n\n    entry.pack(pady=4, fill=tk.X, padx=20)\n    fields[q] = entry\n\n\n# --------------------------------\n# PREDICT BUTTON FUNCTION\n# --------------------------------\ndef predict():\n    try:\n        values = [\n            fields["Gender (1=F, 2=M)"].get(),\n            float(fields["Age"].get()),\n            fields["Smoking"].get(),\n            fields["Yellow Fingers"].get(),\n            fields["Anxiety"].get(),\n            fields["Peer Pressure"].get(),\n            fields["Chronic Disease"].get(),\n            fields["Fatigue"].get(),\n            fields["Allergy"].get(),\n            fields["Wheezing"].get(),\n            fields["Alcohol Consuming"].get(),\n            fields["Coughing"].get(),\n            fields["Shortness of Breath"].get(),\n            fields["Swallowing Difficulty"].get(),\n            fields["Chest Pain"].get()\n        ]\n\n        if "" in values:\n            messagebox.showerror("Error", "Please fill all fields!")\n            return\n\n        input_row = Orange.data.Table(domain, [values])\n        result = model(input_row)[0]\n\n        if result == "2":\n            messagebox.showwarning("Result", " Lung Cancer Likely (YES)")\n        else:\n            messagebox.showinfo("Result", " Lung Cancer Unlikely (NO)")\n\n    except Exception as e:\n        messagebox.showerror("Error", "Something went wrong.\\n" + str(e))\n\n\n# --------------------------------\n# PREDICT BUTTON\n# --------------------------------\ntk.Button(frame, text="Predict", command=predict, font=("Arial", 14),\n          bg="blue", fg="white").pack(pady=20)\n\n\nroot.mainloop()\n\n', 'filename': None}], 'scriptText': '\nimport tkinter as tk\nfrom tkinter import ttk, messagebox\nimport Orange\nimport pickle\n\n# --------------------------------\n# LOAD MODEL\n# --------------------------------\ntry:\n    model = pickle.load(open("model.pkcls", "rb"))\nexcept:\n    messagebox.showerror("Error", "Model file \'lung_model.pkcls\' not found!")\n    exit()\n\n# --------------------------------\n# DOMAIN\n# --------------------------------\ndomain